import java.io.*;
class Twinprime
{
int twin prime(int n1,int n2)
{
int i,j,c1=0,c2=0;
for (i=1,i<=n1,i++)
{
if (n1%i==0)
c1++;
}
for(j=1;j<=n2;j++)
{
if (n2%j==0)
c2++;
if(c1==2&&c2==2&&(n1-n2==2||n2-n1==2)
return(1);
else 
return(0);
}
public static void main (String args[])
{
int n,m,k;
twinprime obj = new Twinprime();
System.out println("enter any two numbers");
}
}
   Output:
           (3,5), (5,7), (11,13)
